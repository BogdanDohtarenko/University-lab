#include <iostream>
#include <math.h>

using namespace std;
int main()
{
	double x,y,z,a,b,c,s;
	cout << "Vvedite x:";
	cin >> x;
	cout << "Vvedite y:";
	cin >> y;
	cout << "Vvedite z:";
	cin >> z;
	a = log(pow(y, -pow(fabs(x), 1.0 / 2)));
	b = x - y/2;  
	c = pow(sin(atan(z)), 2);
	s = a*b + c;
	cout << "result s=" << s << endl;
		system("pause");
	return 0;
}
